# LangSmith Configuration
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=

# Publique um prompt no langsmith hub para gerar seu username, depois abra o prompt e vรก no simbolo de cadeado.
USERNAME_LANGSMITH_HUB=

# OpenAI Configuration
